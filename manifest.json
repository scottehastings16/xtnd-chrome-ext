{
  "manifest_version": 3,
  "name": "Data Layer Push Logger",
  "version": "1.0",
  "description": "Logs all data layer pushes to a table.",
  "permissions": [
    "storage",
    "activeTab",
    "scripting"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_icon": "xtnd.png"
    "default_popup": "popup.html"
  }
}
